########################################################################
############ DO NOT TOUCH ##############################################
############ bitrate NX500 PRO #########################################
########################################################################
bitrate_nx500pro_35=(0 c0f21620)
bitrate_nx500pro_40=(0 c0f26220)
bitrate_nx500pro_45=(0 c0f2ae20)
bitrate_nx500pro_50=(0 c0f2fa20)
